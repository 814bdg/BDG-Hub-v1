game.StarterGui:SetCore("SendNotification", {

	Title = "BDG Hub V1",

	Text = "Loading BDG Hub V1, please be patient"

})

wait(5)

local LocalPlayer = game.Players.LocalPlayer or game.Players:FindFirstChild(game.Players.LocalPlayer.Name)

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BDG Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "BDG Hub"})
local Tab = Window:MakeTab({
	Name = "Lumber tycoon 2",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Scripts"
})
Tab:AddButton({
	Name = "Dark X New Gui",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/bdgxs/connect-/main/Connect'))()
  	end    
})
Tab:AddButton({
	Name = "Dark X Old Gui",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/bdgxs/Old-Dark/main/Dark%20Old'))()
  	end    
})